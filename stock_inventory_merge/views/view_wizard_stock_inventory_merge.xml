<odoo>

    <record id="view_wizard_stock_inventory_merge_form" model="ir.ui.view">
        <field name="model">wizard.stock.inventory.merge</field>
        <field name="arch" type="xml">
            <form>
                <group col="4">
                    <field name="name" />
                </group>
                <footer>
                    <button
                        name="action_merge"
                        string="Merge Inventories"
                        type="object"
                        class="oe_highlight"
                    />
                    or
                    <button string="Cancel" class="oe_link" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

</odoo>
