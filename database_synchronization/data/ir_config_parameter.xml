<odoo noupdate="1">

    <record id="parameter_host" model="ir.config_parameter">
        <field name="key">database_synchronization.host</field>
        <field name="value">localhost</field>
    </record>

    <record id="parameter_port" model="ir.config_parameter">
        <field name="key">database_synchronization.port</field>
        <field name="value">8069</field>
    </record>

    <record id="parameter_database" model="ir.config_parameter">
        <field name="key">database_synchronization.database</field>
        <field name="value">database</field>
    </record>

    <record id="parameter_login" model="ir.config_parameter">
        <field name="key">database_synchronization.login</field>
        <field name="value">admin</field>
    </record>

    <record id="parameter_password" model="ir.config_parameter">
        <field name="key">database_synchronization.password</field>
        <field name="value">admin</field>
    </record>

</odoo>
